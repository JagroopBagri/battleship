{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "object-curly-newline": "off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-param-reassign": "off"
  },
  "plugins": ["jest"]
}
